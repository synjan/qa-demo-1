{
  "name": "qa-demo-1",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:api": "playwright test e2e-tests/api/",
    "test:e2e:smoke": "playwright test e2e-tests/smoke.spec.ts",
    "test:visual": "playwright test --config=playwright-visual.config.ts",
    "test:visual:update": "playwright test --config=playwright-visual.config.ts --update-snapshots",
    "test:visual:ui": "playwright test --config=playwright-visual.config.ts --ui",
    "test:contracts": "jest --testMatch='**/*.contract.test.ts'",
    "test:contracts:verify": "jest --testMatch='**/provider-verification.test.ts'",
    "test:contracts:publish": "pact-broker publish ./pacts --consumer-app-version=$npm_package_version",
    "test:parallel": "jest --config=jest.config.parallel.js --maxWorkers=50%",
    "test:parallel:ci": "jest --config=jest.config.parallel.js --maxWorkers=4",
    "test:e2e:parallel": "playwright test --config=playwright-parallel.config.ts",
    "test:e2e:shard": "playwright test --config=playwright-parallel.config.ts --shard=$SHARD/$TOTAL_SHARDS",
    "test:split": "node scripts/split-tests.js",
    "test:group": "jest --testPathPattern=$(cat .test-splits/group-$GROUP.json | jq -r '.tests | join(\"|\")')",
    "test:perf": "npm run test:parallel -- --reporters=default --reporters=jest-perf-reporter",
    "test:all": "npm run test:parallel && npm run test:e2e:parallel && npm run test:contracts"
  },
  "dependencies": {
    "@octokit/rest": "^22.0.0",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@types/uuid": "^10.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "gray-matter": "^4.0.3",
    "lucide-react": "^0.525.0",
    "next": "15.4.2",
    "next-auth": "^4.24.11",
    "next-themes": "^0.4.6",
    "openai": "^5.10.1",
    "react": "19.1.1",
    "react-dom": "19.1.0",
    "recharts": "^3.1.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@faker-js/faker": "^9.9.0",
    "@jest/globals": "^29.7.0",
    "@pact-foundation/pact": "^15.0.1",
    "@playwright/test": "^1.54.1",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.9",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "axios": "^1.11.0",
    "eslint": "^9",
    "eslint-config-next": "15.4.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-pact": "^0.11.4",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.0",
    "typescript": "^5"
  }
}
